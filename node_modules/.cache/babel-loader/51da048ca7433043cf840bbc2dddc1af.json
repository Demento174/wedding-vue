AAA,IAAA,EAAA;AAEA,UAAA,YAAA,IAAA,UAAA,CAAA,UAAA;AAEA,UAAA,YAAA,IAAA,UAAA,CAAA,SAAA,GAAA,EAAA;AAEA,UAAA,UAAA,GAAA,UAAA,CAAA,YAAA;AACA;;AAEA,QAAA,MAAA,CAAA,QAAA,CAAA,YAAA,EAAA,YAAA;AAEA;AAEA,KA5BA;AA6BA,IAAA,iBAAA,EAAA,2BAAA,OAAA,EAAA,IAAA,EAAA;AAEA,MAAA,GAAA,CAAA,KAAA,CAAA,UAAA,EAAA,OAAA;AACA,MAAA,GAAA,CAAA,KAAA,CAAA,UAAA,EAAA,IAAA;AACA;AAjCA,GApBA;AAuDA,EAAA,UAAA,EAAA;AACA,IAAA,aAAA,EAAA;AADA;AAvDA,CAAA","sourcesContent":["<template>\n  <div class=\"block_service\">\n    <div class=\"item\">\n      <div class=\"heading\">{{ title }}</div>\n\n      <div class=\"content\">\n        <div class=\"anons\"  v-show=\"!showContent\" v-html=\"anons\"></div>\n        <div class=\"text\" v-show=\"showContent\" v-html=\"content\"></div>\n        <div class=\"header\" v-show=\"anonsList\">\n          <div v-for=\"item in anonsList\">\n            {{ item.title }}\n            <span>{{ item.value }}</span>\n          </div>\n\n        </div>\n      </div>\n\n      <div class=\"more\" v-on:click=\"moreBtn($parent)\" v-text=\"showHideContent(`Показать подробности`)\"></div>\n\n    </div>\n\n    <div class=\"item\">\n      <SliderPhoto_1\n        v-bind:imgs=\"imgs\"\n      />\n      <a v-if=\"btn\" href=\"#\" data-toggle=\"modal\" data-target=\"#modal_hall\" class=\"btn\" v-on:click=\"setServiceInModal(title,goal)\">\n        <button class=\"banquet-view\" tabindex=\"0\">{{ btn.content }}</button>\n      </a>\n    </div>\n    <div class=\"item-bottom\">\n      <div v-show=\"price\" class=\"price\">\n        Цена <span>{{ price }}</span>\n      </div>\n      <div class=\"services-photo\" v-show=\"presentPrice\">\n        <img src=\"/images/icon-photo.png\">\n        Эта услуга идет в подарок<br>\n        При заказе банкета от {{ presentPrice }} руб.\n      </div>\n\n    </div>\n  </div>\n</template>\n\n<script>\nimport SliderPhoto_1 from \"@/components/SliderPhoto_1\";\nimport '@/assets/less/import/service.less'\nimport { bus } from '@/bus.js'\nexport default {\n  props:{\n    // parent:Node,\n    title:String,\n    anons:String,\n    content:String,\n    price:String,\n    imgs:Array,\n    presentPrice:String,\n    anonsList:Array,\n    btn:Object,\n    goal:String\n  },\n  data(){\n\n    return {\n      showContent:false\n    }\n  },\n\n  methods: {\n    showHideContent(value) {\n      return this.showContent?'Скрыть':value\n    },\n    moreBtn(parent)\n    {\n      this.showContent = !this.showContent\n\n      if(this.showContent === false)\n      {\n\n        let theElement = parent.$el;\n        let selectedPosX = 0;\n        let selectedPosY = 0;\n\n        while (theElement != null) {\n\n          selectedPosX += theElement.offsetLeft;\n\n          selectedPosY += theElement.offsetTop-50;\n\n          theElement = theElement.offsetParent;\n        }\n\n        window.scrollTo(selectedPosX,selectedPosY);\n\n      }\n\n    },\n    setServiceInModal: (service,goal)=>{\n\n      bus.$emit('set-hall', service)\n      bus.$emit('set-goal', goal)\n    }\n  },\n  components:{\n    SliderPhoto_1\n  },\n\n}\n</script>\n<style scoped lang=\"less\">\n.header {\n  margin-top: 15px;\n  span{\n    margin-bottom: 15px;\n    display: block;\n    color: #a56e7d;\n    font-size: 18px;\n    text-transform: uppercase;\n    font-family: 'Myriad Pro Bold';\n  }\n}\n.more{\n  cursor: pointer;\n  color: #a56e7d;\n  &:hover{\n    text-decoration: underline;\n  }\n}\n.btn{\n  display: block;\n  max-width: 245px;\n  margin:10px auto;\n\n  button{\n    width: 100%;\n    transition: all 0.5s;\n    color: #fff;\n    background: #db9f9d;\n    font-size: 16px;\n    font-weight: bold;\n    text-align: center;\n    border: 0;\n    padding: 10px 0;\n    text-transform: uppercase;\n  }\n}\n@media (max-width: 720px) {\n  .block_service{\n    flex-direction: column;\n    .item{\n      width: 100%;\n    }\n  }\n}\n\n</style>\n\n\n\n\n\n\n"],"sourceRoot":"src/components"}]}