A,KAAA,GAAA;AAFA,SADA;AAMA,QAAA,IAAA,EAAA,IANA;AAOA,QAAA,YAAA,EAAA,CAPA;AAOA;AACA,QAAA,YAAA,EAAA,EARA;AASA,QAAA,cAAA,EAAA,IATA;AAUA,QAAA,aAAA,EAAA,QAAA,CAAA,eAAA,CAAA,WAAA,IAAA,GAAA,GAAA,CAAA,GAAA,CAVA;AAWA,QAAA,UAAA,EAAA,GAXA;AAYA,QAAA,mBAAA,EAAA;AAZA;AATA,KAAA;AAyBA,GArCA;AAsCA,EAAA,OAAA,EAAA;AACA,IAAA,WAAA,EAAA,uBAAA;AACA,WAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AAAA,QAAA,OAAA,CAAA,WAAA,GAAA,KAAA;AAAA,OAAA;AACA;AAHA,GAtCA;AA2CA,EAAA,OA3CA,qBA2CA;AAAA;;AACA,SAAA,SAAA,CAAA,YAAA;AAEA,UAAA,YAAA,GAAA,KAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA;AACA,UAAA,SAAA,GAAA,KAAA,CAAA,KAAA,CAAA,WAAA,CAAA,OAAA;AAEA,MAAA,SAAA,CAAA,UAAA,CAAA,OAAA,GAAA,YAAA;AACA,MAAA,YAAA,CAAA,UAAA,CAAA,OAAA,GAAA,SAAA;AACA,KAPA;AAQA;AApDA,CAAA","sourcesContent":["<template>\n  <section class=\"wedding-services\" :id=\"id\">\n    <div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-12\">\n          <h2>{{ title }}</h2>\n        </div>\n\n        <div class=\"col-12\" v-show=\"services.length>1\">\n          <div class=\"slider-nav-wrapper\">\n            <swiper @slideChangeTransitionEnd=\"changeSlide()\"\n                class=\"slider-nav-b swiper gallery-thumbs\"  :options=\"swiperOptionThumbs\" ref=\"swiperThumbs\">\n              <swiper-slide class=\"head\"  v-for=\"service in services\" v-html=\"service.title\" >\n\n              </swiper-slide>\n            </swiper>\n            <div class=\"swiper-thumb-prev button-prev\" slot=\"button-prev\" :id=\"`prev_${this.uid}`\"></div>\n            <div class=\"swiper-thumb-next button-next\" slot=\"button-next\" :id=\"`next_${this.uid}`\"></div>\n          </div>\n\n\n        </div>\n\n\n      </div>\n    </div>\n    \n    <div class=\"bg-gray\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <swiper class=\"swiper\" :options=\"swiperOptions\" ref=\"swiperHalls\">\n              <swiper-slide v-for=\"service in services\" >\n                <Service\n                    v-bind:title=\"service.title\"\n                    v-bind:anons=\"service.anons\"\n                    v-bind:content=\"service.content\"\n                    v-bind:price=\"service.price\"\n                    v-bind:imgs=\"service.imgs\"\n                    v-bind:present-price=\"service.presentPrice\"\n                    v-bind:anonsList=\"service.anonsList\"\n                    v-bind:btn=\"service.btn\"\n                    ref=\"Service\"\n                />\n              </swiper-slide>\n            </swiper>\n          </div>\n        </div>\n      </div>\n    </div>\n\n  </section>\n</template>\n\n<script>\nimport { Swiper, SwiperSlide } from 'vue-awesome-swiper'\nimport Service from \"@/components/Service\";\nimport '@/assets/less/import/service.less'\nexport default {\n  props:['title','services','id'],\n  components: {\n    Swiper,\n    SwiperSlide,\n    Service\n  },\n  beforeCreate() {\n    this.uid = Math.floor(Math.random() * Math.floor(99999))\n  },\n  data() {\n\n    return{\n      swiperOptions: {\n\n          simulateTouch:false,\n          loop: true,\n          loopedSlides: 5,\n          spaceBetween: 10,\n\n      },\n      swiperOptionThumbs: {\n        navigation: {\n          prevEl: `#prev_${this.uid}`,\n          nextEl: `#next_${this.uid}`\n          },\n\n          loop: true,\n          loopedSlides: 5, // looped slides should be the same\n          spaceBetween: 10,\n          centeredSlides: true,\n          slidesPerView:document.documentElement.clientWidth>=720?3:1,\n          touchRatio: 0.2,\n          slideToClickedSlide: true\n        },\n\n    }\n  },\n  methods:{\n    changeSlide:function (){\n      this.$refs.Service.forEach((service)=>{service.showContent = false})\n    }\n  },\n  mounted() {\n    this.$nextTick(() => {\n\n      const swiperThumbs = this.$refs.swiperThumbs.$swiper\n      const swiperTop = this.$refs.swiperHalls.$swiper\n\n      swiperTop.controller.control = swiperThumbs\n      swiperThumbs.controller.control = swiperTop\n    })\n  },\n\n}\n\n</script>\n\n<style scoped lang=\"less\">\n.wedding-services {\n  margin-bottom:80px;\n  .slider-nav-wrapper{\n    position: relative;\n    .slider-nav-b {\n      .head {\n        background:#a56e7d;\n        color:#fff;\n        padding:15px 0;\n        font-size:16px;\n        font-family:'Myriad Pro Bold';\n        text-align:center;\n        transition:all 0.5s;\n        text-transform:uppercase;\n        &:hover {\n          background:#ffe2e1;\n          color:#a56e7d;\n          padding:15px 0;\n          margin:0 10px;\n\n          text-align:center;\n          transition:all 0.5s;\n          cursor:pointer;\n        }\n        &.slick-current {\n          background:#f8f8f8;\n          color:#58595e;\n          padding:15px 0;\n          text-align:center;\n        }\n      }\n    }\n    .button-next,.button-prev{\n      position: absolute;\n      top: calc(50% - 25px);\n      left: -50px;\n    }\n    .button-next{\n      left: initial;\n      right: -50px;\n    }\n    @media(max-width: 720px){\n      .button-prev{\n        left: 0;\n      }\n      .button-next{\n        right: 0;\n      }\n    }\n  }\n  .bg-gray{\n    background:#f8f8f8;\n    padding:30px 0 50px 0;\n\n  }\n\n}\n\n\n\n</style>"],"sourceRoot":"src/components"}]}