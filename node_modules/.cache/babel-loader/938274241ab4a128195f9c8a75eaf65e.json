A;AAAA;AAAA;AAAA;AAAA;AAAA,gBAAA,OAAA,0EAAA,SAAA;AACA,gBAAA,OAAA,GAAA,+BAAA;AADA;AAAA,uBAEA,KAAA,CAAA,sFAAA,OAAA,CAAA,CACA;AADA,iBAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,yBAAA,QAAA,CAAA,IAAA,EAAA;AACA,iBAJA,EAKA,IALA,CAKA,UAAA,IAAA,EAAA;AACA;AACA,sBAAA,IAAA,CAAA,OAAA,IAAA,IAAA,EAAA;AACA,oBAAA,KAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,oBAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,IAAA,EAFA,CAGA;AACA,mBAJA,MAIA;AACA,oBAAA,KAAA,CAAA,SAAA;AACA;AACA,iBAdA,CAFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBA;AAlBA,GAnBA;AAuCA,EAAA,OAvCA,qBAuCA;AACA,QAAA,MAAA,GAAA,MAAA,CAAA,QAAA,CAAA,MAAA,CAAA,OAAA,CAAA,eAAA,EAAA,EAAA,CAAA;AACA,SAAA,SAAA,CAAA,MAAA;AACA,GA1CA;AA2CA,EAAA,IAAA,EAAA,MA3CA;AA4CA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA,MADA;AAEA,IAAA,KAAA,EAAA,KAFA;AAGA,IAAA,WAAA,EAAA,WAHA;AAIA,IAAA,cAAA,EAAA,cAJA;AAKA,IAAA,QAAA,EAAA,QALA;AAMA,IAAA,UAAA,EAAA,UANA;AAOA,IAAA,kBAAA,EAAA,kBAPA;AAQA,IAAA,SAAA,EAAA,SARA;AASA,IAAA,YAAA,EAAA,YATA;AAUA,IAAA,GAAA,EAAA,GAVA;AAWA,IAAA,MAAA,EAAA,MAXA;AAYA,IAAA,KAAA,EAAA;AAZA;AA5CA,CAAA","sourcesContent":["<template>\n  <div>\n    <Header v-if=\"data != undefined\"\n        v-bind:items=\"data.menu.items\"\n        v-bind:logo=\"data.menu.logo\"\n        v-bind:phone=\"data.menu.phone\"\n        v-bind:size=\"data.menu.fontsize\"\n    />\n\n    <Intro v-if=\"data != undefined && data.introBlock.hide !== 'true'\"\n        v-bind:id=\"`intro`\"\n        v-bind:title=\"data.introBlock.title\"\n        v-bind:backgroundImg=\"data.introBlock.background\"\n        v-bind:titlecolor=\"data.introBlock.textcolor\"\n    />\n\n    <SliderHalls v-if=\"data != undefined && data.tentBlock.hide !== 'true'\"\n        v-bind:title=\"data.tentBlock.title\"\n        v-bind:id=\"'tent'\"\n        v-bind:halls=\"[{\n                        title:data.tentBlock.title,\n                        guests:data.tentBlock.guests,\n                        minPrice:data.tentBlock.minPrice,\n                        characters:data.tentBlock.characters,\n                        imgs:data.tentBlock.imgs,\n                        file_banket:data.tentBlock.file_banket,\n                        file_furshet:data.tentBlock.file_furshet\n        }]\"\n    />\n\n    <SliderHalls v-if=\"data != undefined\"\n      v-bind:title=\"data.banquetHalls_title\"\n      v-bind:id=\"'halls'\"\n      v-bind:halls=\"data.banquetHalls\"\n    />\n\n    <SliderServices v-if=\"data != undefined\"\n        v-bind:title=\"data.services_title\"\n        v-bind:id=\"'services'\"\n        v-bind:services=\"data.services\"\n    />\n\n    <SliderServices v-if=\"data != undefined\"\n        v-bind:title=\"data.specialOffers_title\"\n        v-bind:id=\"'specialOffers'\"\n        v-bind:services=\"data.specialOffers\"\n    />\n\n    <Presents v-if=\"data != undefined\"\n        v-bind:title=\"data.presents_title\"\n        v-bind:id=\"`presents`\"\n        v-bind:presents=\"data.presents\"\n    />\n\n    <Calculator v-if=\"data != undefined\"\n        v-bind:id=\"`calculator`\"\n        v-bind:title=\"data.calculate.title\"\n        v-bind:guest=\"data.calculate.guests\"\n        v-bind:guest_min=\"data.calculate.guests.min\"\n        v-bind:guest_max=\"data.calculate.guests\"\n        v-bind:checks=\"data.calculate.checks\"\n        v-bind:services=\"data.calculate.services\"\n        v-bind:halls=\"data.banquetHalls\"\n        v-bind:pricePresentMode=\"`month`\"\n    />\n\n    <AdditionalServices v-if=\"data != undefined\"\n        v-bind:id=\"`additionalServices`\"\n      v-bind:title=\"data.additionalServices_title\"\n      v-bind:additionalServices=\"data.additionalServices\"\n    />\n\n    <SliderRow v-if=\"data !== undefined && data.galleryRows !== undefined && data.galleryRows.rows !== undefined\"\n        v-bind:id=\"`gallery`\"\n        v-bind:title=\"data.galleryRows.title\"\n        v-bind:imgs=\"data.galleryRows.rows[0].imgs\"\n    />\n\n    <div v-for=\"(gallery, index) in data.galleryRows.rows\">\n      <script>\n        console.log(1);\n      </script>\n      <SliderRow v-if=\"data != undefined && data.galleryRows != undefined && data.galleryRows.rows != undefined && index !== 0\"\n\n                 v-bind:imgs=\"gallery.imgs\"\n      />\n    </div>\n\n\n    <FormCallback v-if=\"data != undefined\"\n        v-bind:id=\"`callback`\"\n        v-bind:title=\"data.callback.title\"\n        v-bind:subtitle=\"data.callback.subtitle\"\n        v-bind:offers=\"data.callback.offers\"\n    />\n\n    <Map v-if=\"data != undefined\"\n        v-bind:id=\"`map`\"\n      v-bind:title=\"data.map.title\"\n      v-bind:coords=\"data.map.coords\"\n    />\n\n    <Footer v-if=\"data != undefined\"\n      v-bind:address=\"data.footer.address\"\n      v-bind:phone=\"data.footer.phone\"\n      v-bind:hours=\"data.footer.hours\"\n      v-bind:email=\"data.footer.email\"\n      v-bind:social=\"data.footer.social\"\n    />\n    <Modal/>\n  </div>\n</template>\n\n<script>\n\nimport Header from '@/components/Header.vue'\nimport Intro from \"@/components/Intro\";\nimport SliderHalls from \"@/components/SliderHalls\";\nimport SliderServices from \"@/components/SliderServices\";\nimport Presents from \"@/components/Presents\";\nimport Calculator from \"@/components/Calculator\";\nimport AdditionalServices from \"@/components/AdditionalServices\";\nimport SliderRow from \"@/components/SliderRow\";\nimport FormCallback from \"@/components/FormCallback\";\nimport Map from \"@/components/Map\";\nimport Footer from \"@/components/Footer\";\nimport Modal from \"@/components/Modal\"\n\nexport default {\n  data() {\n    return {\n      data : undefined,\n      orders: [\n              'intro_order',\n              'site_tent_wedding_order',\n        'bankets_order',\n        'wedding_service_order',\n        'special_proposition_order',\n        'presents_block_order',\n        'calculator_order',\n        'more_service_order',\n        'gallery_order',\n        'callback_order',\n        'map_order']\n\n    }\n  },\n  methods : {\n    async fetchData($domain = 'default') {\n      $domain = 'wedding.russkiydom.beget.tech'\n      await fetch('http://api-wedding.russkiydom.beget.tech/wp-json/wedding/v1/domain-data/?arg_str=' + $domain)\n      // await fetch('http://wedding-api.com/wp-json/wedding/v1/domain-data/?arg_str=' + $domain)\n              .then((response) => {\n                return response.json()\n              })\n              .then((json) => {\n                // console.log(json)\n                if (json.success == true) {\n                  this.data = json.data\n                  console.log(this.data);\n                  // this.orders = json.data.orders\n                } else {\n                  this.fetchData()\n                }\n              })\n      }\n  },\n  created() {\n    let domain = window.location.origin.replace(/(^\\w+:|^)\\/\\//, '');\n    this.fetchData(domain);\n  },\n  name: 'Home',\n  components: {\n    Header,\n    Intro ,\n    SliderHalls,\n    SliderServices,\n    Presents,\n    Calculator,\n    AdditionalServices,\n    SliderRow,\n    FormCallback,\n    Map,\n    Footer,\n    Modal,\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}