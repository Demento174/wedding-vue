AA,IAAA,GAAA,GAAA,CAAA,GAAA,CALA;AAMA,QAAA,UAAA,EAAA,GANA;AAOA,QAAA,mBAAA,EAAA,IAPA;AAQA,QAAA,UAAA,EAAA;AACA,UAAA,MAAA,kBAAA,KAAA,GAAA,CADA;AAEA,UAAA,MAAA,kBAAA,KAAA,GAAA;AAFA;AARA;AAPA,KAAA;AAsBA,GAjCA;AAkCA,EAAA,OAlCA,qBAkCA;AAAA;;AACA,SAAA,SAAA,CAAA,YAAA;AAEA,UAAA,YAAA,GAAA,KAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA;AACA,UAAA,SAAA,GAAA,KAAA,CAAA,KAAA,CAAA,WAAA,CAAA,OAAA;AAEA,MAAA,SAAA,CAAA,UAAA,CAAA,OAAA,GAAA,YAAA;AACA,MAAA,YAAA,CAAA,UAAA,CAAA,OAAA,GAAA,SAAA;AACA,KAPA;AAQA;AA3CA,CAAA","sourcesContent":["<template>\n  <section class=\"banquet-halls\" :id=\"id\">\n    <div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-12\">\n          <h2>{{ title }}</h2>\n        </div>\n\n        <div class=\"col-12\" v-show=\"halls.length>1\">\n          <div class=\"slider-nav-wrapper\">\n            <swiper class=\"slider-nav-b swiper gallery-thumbs\" :options=\"swiperOptionThumbs\" ref=\"swiperThumbs\">\n\n              <swiper-slide  v-for=\"(hall,index) in halls\" v-html=\"hall.title\" :class=\"`head slide-${index}`\">\n\n              </swiper-slide>\n\n            </swiper>\n            <div class=\"swiper-thumb-prev button-prev\" slot=\"button-prev\" :id=\"`prev_${this.uid}`\"></div>\n            <div class=\"swiper-thumb-next button-next\" slot=\"button-next\" :id=\"`next_${this.uid}`\"></div>\n          </div>\n        </div>\n\n      </div>\n    </div>\n\n    <div class=\"bg-gray\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n\n            <swiper class=\"swiper\" :options=\"swiperOptions\" ref=\"swiperHalls\">\n              <swiper-slide v-for=\"(hall,index) in halls\" :class=\"`slide-${index}`\">\n                    <Hall\n                        v-bind:title=\"hall.title\"\n                        v-bind:guests=\"hall.guests\"\n                        v-bind:minPrice=\"hall.minPrice\"\n                        v-bind:characters=\"hall.characters\"\n                        v-bind:imgs=\"hall.imgs\"\n                        v-bind:file_banket=\"hall.file_banket\"\n                        v-bind:file_furshet=\"hall.file_furshet\"\n\n                        v-bind:goal=\"goal\"\n                    />\n              </swiper-slide>\n            </swiper>\n\n          </div>\n        </div>\n      </div>\n    </div>\n\n  </section>\n</template>\n\n<script>\nimport { Swiper, SwiperSlide } from 'vue-awesome-swiper'\nimport Hall from \"@/components/Hall\";\nexport default {\n  props:['title','halls','id','goal'],\n  components: {\n    Swiper,\n    SwiperSlide,Hall\n  },\n  beforeCreate() {\n    this.uid = Math.floor(Math.random() * Math.floor(99999))\n  },\n  data() {\n\n    return{\n      swiperOptions: {\n          loop: true,\n          loopedSlides: 5,\n          spaceBetween: 10,\n          simulateTouch:false,\n      },\n      swiperOptionThumbs: {\n          loop: true,\n          loopedSlides: 5, // looped slides should be the same\n          spaceBetween: 10,\n          centeredSlides: true,\n          slidesPerView: document.documentElement.clientWidth>=720?3:1,\n          touchRatio: 0.2,\n          slideToClickedSlide: true,\n          navigation: {\n            nextEl: `#next_${this.uid}`,\n            prevEl: `#prev_${this.uid}`\n          },\n\n        },\n    }\n  },\n  mounted() {\n    this.$nextTick(() => {\n\n      const swiperThumbs = this.$refs.swiperThumbs.$swiper\n      const swiperTop = this.$refs.swiperHalls.$swiper\n\n      swiperTop.controller.control = swiperThumbs\n      swiperThumbs.controller.control = swiperTop\n    })\n  }\n}\n</script>\n\n<style scoped lang=\"less\">\n  .slider-nav-wrapper{\n    position: relative;\n    .button-next,.button-prev{\n      position: absolute;\n      top: calc(50% - 25px);\n      left: -50px;\n    }\n    .button-next{\n      left: initial;\n      right: -50px;\n    }\n    @media(max-width: 720px){\n      .button-prev{\n        left: 0;\n      }\n      .button-next{\n        right: 0;\n      }\n    }\n  }\n</style>"],"sourceRoot":"src/components"}]}